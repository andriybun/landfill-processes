keywords = {Computer Simulation,Models, Theoretical,Monte Carlo Method,Poland,Porosity,Refuse Disposal,Refuse Disposal: methods,Soil,Soil: analysis,Soil: chemistry,Stochastic Processes,Time Factors,Water Movements,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis,Water Pollution,Water Pollution: prevention \& control},
month = jan,
pages = {489--96},
pmid = {23194922},
title = {{Stochastic approach to municipal solid waste landfill life based on the contaminant transit time modeling using the Monte Carlo (MC) simulation.}},

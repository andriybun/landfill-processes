month = oct,
number = {10},
pages = {1969--81},
pmid = {23318154},
publisher = {Elsevier Ltd},
title = {{Developments to a landfill processes model following its application to two landfill modelling challenges.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23318154},
volume = {33},
year = {2013}
}

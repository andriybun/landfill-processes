keywords = {Biochemical Processes,Biodegradation, Environmental,Cities,Forecasting,Mechanical Processes,Models, Chemical,Refuse Disposal,Waste Products,Waste Products: analysis},
number = {8-9},
pages = {1556--68},
pmid = {20381332},
publisher = {Elsevier Ltd},
title = {{Decoupling MSW settlement into mechanical and biochemical processes--modelling and validation on large-scale setups.}},

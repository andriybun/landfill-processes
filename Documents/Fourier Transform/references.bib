doi = {10.1016/j.wasman.2012.12.006},
file = {:home/andriybun/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/White, Beaven - 2013 - Developments to a landfill processes model following its application to two landfill modelling challenges.pdf:pdf},
